    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.21",
    "husky": "^8.0.3",
    "jest": "^29.6.3",
    "jest-environment-jsdom": "^29.6.3",
    "jest-transform-css": "^6.0.1",
    "react": "^18.2.0",
    "release-it": "^16.1.5",
    "ts-jest": "^29.1.1",
